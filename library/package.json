    "build:esm": "tsc --p ./esm/tsconfig.json"
  },
  "dependencies": {
    "@taiko-wiki/taikowiki-api": "^1.0.1",
    "openai": "^5.7.0"
  },
  "devDependencies": {
    "marked": "^15.0.12",
    "@types/node": "^24.0.4"
  }
}